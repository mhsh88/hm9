<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AddPerson</servlet-name>
  	<display-name>AddPerson</display-name>
  	<description></description>
  	<servlet-class>api.Person.add.AddPerson</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetPerson</servlet-name>
  	<display-name>GetPerson</display-name>
  	<description></description>
  	<servlet-class>api.Person.get.GetPerson</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeletePerson</servlet-name>
  	<display-name>DeletePerson</display-name>
  	<description></description>
  	<servlet-class>api.Person.deletePerson.DeletePerson</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetAll</servlet-name>
  	<display-name>GetAll</display-name>
  	<description></description>
  	<servlet-class>api.Person.getAll.GetAll</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdatePerson</servlet-name>
  	<display-name>UpdatePerson</display-name>
  	<description></description>
  	<servlet-class>api.Person.updatePerson.UpdatePerson</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddPerson</servlet-name>
  	<url-pattern>/api/Person/AddPerson</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetPerson</servlet-name>
  	<url-pattern>/api/Person/GetPerson</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeletePerson</servlet-name>
  	<url-pattern>/api/Person/deletePerson</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetAll</servlet-name>
  	<url-pattern>/api/Person/GetAll</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdatePerson</servlet-name>
  	<url-pattern>/api/Person/updatePerson</url-pattern>
  </servlet-mapping>
  

</web-app>
